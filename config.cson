"*":
  "atom-typescript":
    disableAtomIdeDefinitions: true
    preferBuiltinBusySignal: true
    preferBuiltinOccurrenceHighlight: true
    preferBuiltinSigHelp: true
    preferBuiltinTooltips: true
  core:
    allowPendingPaneItems: false
    customFileTypes:
      "source.ini": [
        ".buckconfig"
        ".hgrc"
        ".flowconfig"
      ]
      "source.js": [
        "ds"
        "tss"
      ]
      "source.js.jsx": [
        "jsx"
        "js"
        "tsx"
      ]
      "source.json": [
        ".arcconfig"
        "BUCK.autodeps"
      ]
      "source.python": [
        "BUCK"
      ]
    disabledPackages: [
      "tool-bar"
      "haskell-grammar"
      "highlight-selected"
      "sort-lines"
      "set-syntax"
      "language-ini"
      "language-lua"
      "language-ocaml"
      "language-swift"
      "language-thrift"
      "language-graphql"
      "MagicPython"
      "appcelerator-titanium"
      "nuclide-format-js"
      "atom-ide-ui"
      "react"
      "image-view"
      "symbols-tree-view"
      "minimap-autohider"
    ]
    telemetryConsent: "no"
    themes: [
      "one-dark-ui"
      "atom-rails-envy-dark"
    ]
  editor:
    fontSize: 16
    preferredLineLength: 1000
    tabLength: 4
    tabType: "soft"
  "exception-reporting":
    userId: "f967d3b0-d2ce-44f7-86bb-d7ada58de045"
  jsformat:
    format_on_save: false
  "language-babel":
    autoIndentJSX: true
    keepFileExtension: true
  linter: {}
  "linter-eslint":
    autofix:
      fixOnSave: true
  "linter-jshint": {}
  "linter-ui-default":
    alwaysTakeMinimumSpace: true
    panelHeight: 69
  minimap:
    plugins:
      bookmarks: true
      bookmarksDecorationsZIndex: 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      linter: true
      linterDecorationsZIndex: 0
      "minimap-autohider": false
      "minimap-autohiderDecorationsZIndex": 0
  nuclide:
    "atom-ide-diagnostics-ui": {}
    installRecommendedPackages: true
    "nuclide-file-tree":
      revealFileOnSwitch: true
    "nuclide-home":
      showHome: false
    use:
      "atom-ide-busy-signal": "default"
      "atom-ide-code-actions": "default"
      "atom-ide-code-format": "default"
      "atom-ide-code-highlight": "default"
      "atom-ide-console": "default"
      "atom-ide-datatip": "default"
      "atom-ide-debugger": "default"
      "atom-ide-debugger-java": "default"
      "atom-ide-debugger-native-gdb": "default"
      "atom-ide-debugger-node": "default"
      "atom-ide-debugger-ocaml": "default"
      "atom-ide-debugger-python": "default"
      "atom-ide-debugger-react-native": "default"
      "atom-ide-definitions": "default"
      "atom-ide-diagnostics": "default"
      "atom-ide-diagnostics-ui": "default"
      "atom-ide-find-references": "default"
      "atom-ide-global": "default"
      "atom-ide-outline-view": "default"
      "atom-ide-signature-help": "default"
      "atom-ide-terminal": "default"
      hyperclick: "default"
      "nuclide-adb": "default"
      "nuclide-adb-logcat": "default"
      "nuclide-adb-sdb": "default"
      "nuclide-atom-notifications": "default"
      "nuclide-autocomplete": "default"
      "nuclide-blame": "default"
      "nuclide-blame-provider-hg": "default"
