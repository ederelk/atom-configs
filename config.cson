  "platformio-ide-terminal":
    style:
      theme: "homebrew"
    toggles:
      autoClose: true
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    scrollSyncType: "Vertical + Horizontal"
  "symbols-tree-view":
    defaultWidth: 300
  "tree-view":
    alwaysOpenExisting: true
    autoReveal: true
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
".reason.source":
  editor:
    preferredLineLength: 110
